 <div class="tab spend-tab" data-group="primary" data-tab="spend-tab">
    <!-- Body -->
    <div class="lp-history__spend-body">
      <table class="input--centered">
        <thead>
          <tr>
            <th>
              {{localize "ED.Actor.LpTracking.Header.date"}}
            </th>
            <th>
              {{localize "ED.Actor.LpTracking.Header.type"}}
            </th>
            <th>
              {{localize "ED.Actor.LpTracking.Header.name"}}
            </th>
            <th>
              {{localize "ED.Actor.LpTracking.Header.description"}}
            </th>
            <th>
              {{localize "ED.Actor.LpTracking.Header.legendPoints"}}
            </th>
          </tr>
        </thead>
        <tbody>
          {{#each this.object.spendings}}
          <tr>
            <td>
              {{this.date}}
            </td>
            <td>
              {{this.entityType}}
            </td>
            <td>
              {{this.name}}
            </td>
            <td>
              {{this.description}}
            </td>
            <td>
              {{this.amount}}
            </td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
 </div>